*
	margin 0
	padding 0
	line-height 1
	// box-sizing border-box
	-webkit-font-smoothing anitialiased
ul
ol
	list-style none
a
	text-decoration none
	color inherit
em
	font-style normal
a
img
	border 0
body
	font-family "microsoft yahei"
	font-size 12px
.header
	margin 0 auto
	background-color #f5f5f5
	width 100%
	height 74px
	min-width 990px
	font-size 12px
	line-height 1.5
	font-family tahoma arial sans-serif
	.header-main
		margin 0 auto
		width 980px
		height 36px
		position relative
		.logo
			a
				width 130px
				height 36px
				float left
				background url("../img/header.png") -33px -137px no-repeat
				text-decoration none
				color #555
				outline-width 0px
		.header-nav
			float right
			display inline
			padding-top 10px
			width 688px
			height 14px
			ul
				float right
				display inline
				line-height 14px
				li
					float left
					display inline
					padding 0 11px 0 10px
					background url("../img/header.png") 100% -98px no-repeat
					// background-position 100% -100\0
					a
						text-decoration none
						color #666
						&:hover
							color rgb(0 163 210)
				.username
					padding-right 0px
					background 0
					span
						margin-right 5px
					a
						color #0084bb
				.register
					padding-left 5px
					span
						margin-right 5px
					a
						color #0084bb
				.message
					min-width 76px
					position relative
					left -1px
					top -1px
					height 19px
					border 0 solid black
					padding-left 0
					padding-right 0
					display inline-block
					.message-inner
						padding 5px 10px 0 10px
						margin-top -5px
						background 0
						height 19px
						bottom -1px
						border-left 1px solid #fff
						border-right 1px solid #fff
						position relative
						color #666
						i
							position relative
							background url("../img/icon.png") -30px -153px no-repeat
							width 11px
							height 8px
							display inline-block
							overflow-x hidden
							margin-right 5px
						b
							font-weight bold
							display inline-block
							font-family tahoma
							color #f55
							vertical-align baseline
						.message-inner-arrow
							display inline-block
							position relative
							left 0px
							right 10px
							width 6px
							height 6px
							background url("../img/header.png") -153px -21px no-repeat
							overflow hidden
				.omenu
					margin 0 0 -5px -1px
					padding 0 1px 0 0
					&:hover
						dt
							position relative
							top -5px
							margin 0 0 -4px
							border-color #d6d6d6
							border-bottom 0 none
							background-color #fff
							z-index 5
						dd
							display block
					dl
						position relative
						dt
							position relative
							top -4px
							margin 0 0 -4px 1px
							padding 0 5px
							border 1px solid transparent
							.omenu-link
								display block
								.omenu-txt
									display block
									margin-bottom -5px
									padding 4px 17px 5px 5px
									cursor pointer
									b
										position absolute
										top 8px
										right 10px
										display block
										width 7px
										height 4px
										background url("../img/header.png") -152px -21px no-repeat
										overflow hidden
						dd
							position absolute
							top 16px
							left 0px
							display none
							padding 0 0 5px
							border 1px solid #d6d6d6
							background-color #fff
							z-index 2
							.item
								padding-left 5px
								padding-right 5px
								a
									display block
									padding 0 5px
									line-height 22px
									white-space nowrap
									word-break break-all
									background-color #fff
									text-align left
									color #666
				.last
					padding-right 0
					background 0
		.header-mini-nav
			position absolute
			left 135px
			height 36px
			.mini-txt
				height 36px
				line-height 36px
				display inline-block
				zoom 1
				color #666
				margin-right 3px
				&:hover
					color rgb(0 163 210)
			.mnav
				height 100%
				position relative
				padding 0 10px
				width 78px
				display inline-block
				zoom 1
				.left
				.right
					position absolute
					width 2px
					font-size 0
					bottom 9px
					background url("../img/header.png") 100% -102px no-repeat
					height 12px
				&
					.left
						left 0
					.right
						right
				.lst
					cursor default
					display block
					margin-top 11px
					line-height 14px
					em
						background url("../img/header.png") 0 -154px no-repeat
						font-size 0
						width 13px
						height 10px
						position absolute
						left 10px
						top 14px
					span
						padding-left 17px
					i
						font-size 0
						vertical-align middle
						width 7px
						height 4px
						background url("../img/header.png") -152px -21px no-repeat
						position absolute
						right 10px
						top 18px
	.p_nav
		background-color #00bcd4
		.p_nav_ct
			width 990px
			height 38px
			margin 0 auto
			.icon
				width 14px
				height 16px
				font 1px/1px arial
				display block
				position absolute
				top 4px
				right 9px
				background url("../img/icon2.png") 0 -26px
			.deactve
				float left
				display inline
				position relative
				white-space nowrap
				height 38px
				a
					padding 0 20px
					height 38px
					font 700 14px/38px arial
					color #fff
					display inline-block
					&:hover
						background-color #555
			.agio
				.icon
					background-position 0 -52px
			.haiwai
				.icon
					background-position 0 0
			.jiu
				.icon
					background-position 0 -78px
			.bao
				.icon
					width 25px
					background url("../img/icon3.png") 0 -128px
					right 0
					
.wrap
	min-width 980px
	font-size 14px
	line-height 1.5
	font-family "microsoft yahei" "sans-serif"
.wrap-topnav
	position relative
	height 60px
	.topnav-wrapper
		position absolute
		top 0
		left 0
		width 100%
		height 100%
		min-width 980px
		background-color #242424
		color #fff
		z-index 10
		.m-topnav
			zoom 1
			width 980px
			margin 0 auto
			.m-topnav-tit
				position relative
				width 70px
				float left
				.index
					display inline-block
					width 26px
					height 60px
					background-image url("../img/index.png")
					background-repeat none
				.text
					display inline-block
					position absolute
					top 10px
					left 30px
					width 42px
					text-align center
					font-size 18px
					line-height 20px
					font-weight bold
					color #fe0
			.cont
				float left
				padding 15px 0 0 3px
				width 907px
				font-size 0
				.item
					display inline-block
					padding 0 3px
					line-height 1
					font-size 20px
					.city
						display inline-block
						padding 0 10px
						line-height 26px
						color #fff
						border 2px solid #fff
						border-radius 15px
						font-size 16px
						&:hover
							color #fee648
							border-color #fee648
							background-color #242424
					.color
						background-color #fee648
						border-color #fee648
						color #242424
						font-weight bold
.wrap-top
	position relative
	background-color rgb(254 223 10)
	padding-top 520px
	overflow hidden
	.cf
		zoom 1
	.m-top
		position absolute
		top 0
		left 50%
		margin-left -960px
		width 1920px
		img
			vertical-align middle
		.m-top-txt
			position absolute
			left 50%
			top 244px
			width 366px
			margin-left -183px
			line-height 44px
			font-size 28px
			font-weight bold
			text-align center
			color #fff
	.m-box
		width 980px
		margin 0 auto
		padding-bottom 20px
	.m-time
		width 960px
		position absolute
		left 50%
		top 443px
		margin-left -483px
		.item
			float left
			width 320px
			text-align center
			font-weight bold
			line-height 1
			color #d1afff
			.tit
				font-size 28px
			.desc
				margin-top 3px
				font-size 20px
		.item-color
			color #fffca5
	.m-quan
		width 980px
		margin 0 auto
		padding-top 20px
		.list
			width 990px
			.item
				float left
				margin 0 1px 1px 0
				height 80px
				width 244px
				background-color #f30038
				border-radius 10px
				overflow hidden
				color #fff
				cursor pointer
				font-size 0
				zoom 1
				&:after
					display block
					overflow hidden
					clear both
					height 0
					content "\0020"
				.price
					float left
					padding-left 15px
					line-height 110px
					font-weight bold
					.yen
						font-size 16px
						font-family microsoft yahei
					.va
						font-size 42px
				.type
					position relative
					z-index 2
					float left
					width 40px
					padding-top 20px
					margin-left 5px
					line-height 20px
					font-size 18px
					font-weight bold
				.btn
					position relative
					z-index 1
					float right
					top 0
					width 46px
					height 60px
					padding 20px 10px 0 0
					font-size 18px
					line-height 18px
					text-align center
					background-color #da002b
					&:after
						position absolute
						left -42px
						top 0
						content ''
						width 45px
						height 80px
						background url("../img/sp.png") 0 -60px no-repeat
			.item_l
				.type
					width 60px
			.item_d
				.btn
					&:after
						background-position -46px -60px
			.item_t
				.btn
					&:after
						background-position -92px -60px
			.item_e
				.btn
					&:after
						background-position -184px -60px
	.m-bnr
		margin-top 162px
		.main-inner
			position relative
			&:after
				content '广告'
