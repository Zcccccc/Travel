		ul
			height 227px
			padding 3px 0 0 124px
			background url("../img/bg01.jpg") 50% 0 no-repeat
			border-radius 10px
			.item
				float left
				width 206px
				height 66px
				margin 7px 7px 0 0
				background-color #fff
				border-radius 10px
				overflow hidden
				.abox
					display block
					position relative
					padding 7px 10px 7px 36px
					height 52px
					.tit
						font-size 22px
						font-weight bold
						color #f45a2a
						line-height 32px
						white-space nowrap
						overflow hidden
						text-overflow ellipsis
					.desc
						font-size 14px
