				height 167px
				margin-right 7px
				background-color #ccc
				border-radius 10px
				overflow hidden
				.abox
					display block
					height 165px
					position relative
					img
						vertical-align middle
					.info
						position absolute
						left 20px
						top 12px
						width 250px
						color #fff
						.tit
							font-size 18px
						.desc
							margin-top 8px
